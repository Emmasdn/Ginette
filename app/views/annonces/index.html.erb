<h1 class="text-center"> ALl the nannies available</h1>


<div class="container">
  <div class="row">
    <% @annonces.each do |annonce| %>
    <div class="col-xs-12 col-sm-4">
      <h2><%= annonce.name %></h2>
      <p><%= annonce.city %></p>
      <p><%= annonce.price %></p>
      <p><%= annonce.child_number %></p>
      <%= link_to "More", annonce_path(annonce) %>
    </div>
    <% end %>
  </div>
</div>
