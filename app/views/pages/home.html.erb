<h1>Pages#home</h1>
<p>Find me in app/views/pages/home.html.erb</p>


<div class="col-xs-10 col-xs-offset-1">
  <%= form_tag(annonces_path, method: :get) do %>
    <div class="form-group">
      <%= label_tag :city %>
      <%= text_field_tag :city, nil, placeholder: "City", class: "form-control" %>
    </div>
    <div class="form-group">
      <%= label_tag :date %>
      <%= text_field_tag :date, nil, placeholder: "DD/MM/YYYY", class: "form-control" %>
    </div>
    <div class="form-group">
      <%= label_tag :number_child %>
      <%= text_field_tag :number_child, nil, placeholder: "Number of children", class: "form-control" %>
    </div>
    <%= submit_tag "Search", class: "btn btn-primary" %>
  <% end %>
</div>
